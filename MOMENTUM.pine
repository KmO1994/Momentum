VALUATE( Series )=>
	floorvalue				= 0.0
	posvalue				= 0.0
	negvalue				= 0.0
	
	for i= 1 to 32 // 1 to and 24, is to 25
		a					= PIXEL( Series , i )
		posvalue := posvalue + ( (a>0?a:0) * GRADE(i) )
		negvalue := negvalue + ( (a<0?a:0) * GRADE(i) )
		
	floorvalue				:= PIXEL( close , 32 ) > 0 ? pow(posvalue, 2) : - pow(negvalue, 2)

